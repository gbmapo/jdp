{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/advpoll": "^1.2",
        "drupal/backup_migrate": "^5.0",
        "drupal/colorbox": "^2.0",
        "drupal/config_split": "^1.7",
        "drupal/core-composer-scaffold": "9.5.3",
        "drupal/core-dev": "^9",
        "drupal/core-project-message": "9.5.3",
        "drupal/core-recommended": "9.5.3",
        "drupal/csv_serialization": "^2.0",
        "drupal/devel": "^5.0",
        "drupal/fontawesome": "^2.22",
        "drupal/fullcalendar_view": "^5.1",
        "drupal/honeypot": "^2.0",
        "drupal/imce": "^2.4",
        "drupal/mail_safety": "^1.2",
        "drupal/mailsystem": "^4.2",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/phpmailer_smtp": "2.0.5",
        "drupal/poll": "^1.3",
        "drupal/potx": "^1.0@alpha",
        "drupal/r4032login": "^2.1",
        "drupal/superfish": "^1.3",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/upgrade_status": "^3.5",
        "drupal/views_accordion": "^2.0",
        "drupal/views_data_export": "^1.1",
        "drupal/views_send": "^1.4",
        "drush/drush": "^9.7.1 | ^10.0.0",
        "kint-php/kint": "^4.2",
        "vlucas/phpdotenv": "^4.0",
        "webflo/drupal-finder": "^1.0.0",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        }
    }
}
