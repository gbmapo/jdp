{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/backup_migrate": "^5.0",
        "drupal/colorbox": "^2.0",
        "drupal/config_split": "^1.7",
        "drupal/core-composer-scaffold": "^10.2",
        "drupal/core-dev": "^10.2",
        "drupal/core-project-message": "^10.2",
        "drupal/core-recommended": "^10.2",
        "drupal/csv_serialization": "^4.0",
        "drupal/devel": "^5.0",
        "drupal/fullcalendar_view": "^5.1",
        "drupal/honeypot": "^2.0",
        "drupal/imce": "^3.0",
        "drupal/mail_safety": "^2.0",
        "drupal/mailsystem": "^4.2",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/phpmailer_smtp": "^2.1",
        "drupal/poll": "^1.3",
        "drupal/potx": "^1.0@alpha",
        "drupal/r4032login": "^2.1",
        "drupal/redirect": "^1.9",
        "drupal/superfish": "^1.3",
        "drupal/token": "^1.13",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/upgrade_status": "^4.1",
        "drupal/views_accordion": "^2.0",
        "drupal/views_data_export": "^1.4",
        "drupal/views_send": "^2.0@RC",
        "drush/drush": "^12.4.3",
        "kint-php/kint": "^4.2",
        "vlucas/phpdotenv": "^4.0",
        "webflo/drupal-finder": "^1.0.0",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "zaporylie/composer-drupal-optimizations": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        }
    }
}
